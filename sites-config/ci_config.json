{
  "artifactStructure": {
    "assets": [
      {
        "root": "storybook-static"
      }
    ],
    "hbsTemplates": []
  },
  "dependencies": {
    "installDepsCmd": "echo $npm_config_auth && npm install",
    "requiredFiles": ["package.json", "package-lock.json"]
  },
  "buildArtifacts": {
    "buildCmd": "npm run build-storybook"
  },
  "livePreview": {
      "serveCmd": "npm run storybook -- -p 8080"
  }
}